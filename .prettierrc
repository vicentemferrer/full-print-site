{
  "plugins": ["prettier-plugin-astro"],
  "jsxSingleQuote": true,
  "singleQuote": true,
  "printWidth": 120,
  "trailingComma": "none",
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    },
    { "files": "*.ts", "options": { "parser": "typescript" } }
  ]
}
